{
  "name": "twitter-openweathermap",
  "version": "1.11.87",
  "private": true,
  "description": "Publish on Twitter data from OpenWeatherMap API",
  "main": "src/app.js",
  "scripts": {
    "start": "node .",
    "dev": "nodemon .",
    "lint": "eslint --ext .js .",
    "test": "mocha test",
    "test:coverage": "nyc mocha test",
    "doc-view": "jsdoc --private -c ./conf_jsdoc.json -r && live-server --host=localhost --port=4400 --open=doc/ --ignore=node_modules/,test/*,src/*,.git/,.",
    "purge": "rm -fr ./doc  && rm -fr ./logs && rm -fr ./.nyc_output"
  },
  "keywords": [
    "Twitter",
    "OpenWeatherMap",
    "bot"
  ],
  "author": "Dídac García (https://didaquis.github.io/)",
  "license": "MIT",
  "engines": {
    "node": "^9.4.0"
  },
  "dependencies": {
    "dotenv": "^6.2.0",
    "log4js": "^3.0.6",
    "md5": "^2.2.1",
    "moment-timezone": "^0.5.26",
    "node-fetch": "^2.6.0",
    "twitter": "^1.7.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "jsdoc": "^3.6.3",
    "live-server": "^1.2.1",
    "mocha": "^6.2.1",
    "nodemon": "^1.19.3",
    "nyc": "^14.1.1",
    "sinon": "^7.5.0"
  }
}
